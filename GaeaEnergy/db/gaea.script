CREATE SCHEMA PUBLIC AUTHORIZATION DBA
CREATE MEMORY TABLE USUARIO(ID_USER INTEGER NOT NULL PRIMARY KEY,NOME VARCHAR(45) NOT NULL,SOBRENOME VARCHAR(45) NOT NULL,EMAIL VARCHAR(120) NOT NULL,SENHA VARCHAR(30) NOT NULL)
CREATE MEMORY TABLE USUARIOFINAL(CPF DECIMAL NOT NULL PRIMARY KEY,TIPOPESSOA INTEGER,CNPJ VARCHAR(45),NOME VARCHAR(45),ENDERECO VARCHAR(90),NUMERO INTEGER,COMPLEMENTO VARCHAR(45),REFERENCIA VARCHAR(45),CEP DECIMAL,BAIRRO VARCHAR(45),CIDADE VARCHAR(90),ESTADO VARCHAR(60),PA\u00cdS VARCHAR(45))
CREATE MEMORY TABLE CONTABANCARIA(NOME VARCHAR(45) NOT NULL PRIMARY KEY,AGENCIA INTEGER NOT NULL,CONTA INTEGER NOT NULL,ABREVIATURA VARCHAR(45) NOT NULL,NUMERO INTEGER NOT NULL,ENDERECO VARCHAR(80) NOT NULL,ESTADO VARCHAR(60) NOT NULL,CEP INTEGER NOT NULL,CIDADE VARCHAR(90) NOT NULL,DV INTEGER NOT NULL,DV_CC INTEGER NOT NULL,DV_AG_CC INTEGER NOT NULL,NOME_AGENCIA VARCHAR(90) NOT NULL,TIPO_DOCUMENTO INTEGER NOT NULL,COD_CONTA INTEGER NOT NULL,CONTABIL INTEGER NOT NULL)
CREATE MEMORY TABLE CIDADE(CIDADE VARCHAR(90) NOT NULL PRIMARY KEY,ESTADO VARCHAR(75))
CREATE USER SA PASSWORD ""
GRANT DBA TO SA
SET WRITE_DELAY 10
